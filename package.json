{
  "name": "arenatv",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.1.0",
    "he": "^1.1.1",
    "history": "^4.6.1",
    "re-base": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-favicon": "^0.0.13",
    "react-player": "^1.1.2",
    "react-router-dom": "^4.2.2",
    "react-url-query": "^1.4.0",
    "youtube-node": "^1.3.2"
  },
  "description": "Weekly Are.na video publication",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint ./",
    "start-js": "react-scripts start",
    "build-css": "node-sass src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass src/styles -o src/styles --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom"
  },
  "keywords": [
    "are.na",
    "api",
    "youtube",
    "vimeo",
    "video",
    "playlist"
  ],
  "author": "Gavin Atkinson <gavin@gav.space",
  "contributors": [
    "Callil Capuozzo"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ]
  },
  "license": "MIT"
}
